(define primitive-procedures
  (list (list 'car car)
        (list 'cdr cdr)
        (list 'cadr cadr)
        (list 'cons cons)
        (list 'list list)
        (list 'null? null?)
        (list 'equal? equal?)
        (list 'write-line write-line)
        (list '* *)
        (list '+ +)
        (list '- -)
        (list '= =)
        (list '/ /)))
