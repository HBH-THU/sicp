;;(load "eval.scm")
(load "no-operands?.scm")
(load "first-operand.scm")
(load "rest-operands.scm")

(define (list-of-values exps env)
  (if (no-operands? exps)
      '()
      (cons (eval (first-operand exps) env)
            (list-of-values (rest-operands exps) env))))
