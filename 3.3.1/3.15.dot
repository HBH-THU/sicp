        z1 -> z1_box:0
        z1_box:0 -> a_z1_box:0 -> a1
        x -> a_z1_box:0
        z1_box:1 -> a_z1_box:1
        a_z1_box:1 -> b_z1_box:0
        b_z1_box:0 -> b1
