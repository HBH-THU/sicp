        {
                node[shape=record]
                z1_box[label="<0>|<1>"]
                a_z1_box[label="<0>|<1>"]
                b_z1_box[label="<0>|<1>"]
//                 a_x_box[label="<0>|<1>"]
//                 b_y_box[label="<0>|<1>"]
                z2_box[label="<0>|<1>"]
                a1_z2_box[label="<0>|<1>"]
